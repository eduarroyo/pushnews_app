"#menuView":{
	layout: 'vertical',
	backgroundColor: 'white'
},
"#menuTopBar[if=Alloy.Globals.isIOS7Plus]": {
	height: "100dp",
},
"#menuTopBar[if=!Alloy.Globals.isIOS7Plus]": {
	height: "90dp",
},
"#menuTopBar[platform=android]": {
	height: "90dp",
},
"#menuTopBar":{
	width: Ti.UI.FILL,
//	backgroundGradient: {
//        type: 'linear',
//        colors: [ '#d9885d', '#ba513e'],
//        backfillStart: true
//	},
	layout: 'vertical'
},
"#viewEscudo[if=Alloy.Globals.isIOS7Plus]": {
	top: "10dp"
},
"#viewEscudo[if=!Alloy.Globals.isIOS7Plus]": {
	top: "0dp"
},
"#viewEscudo[platform=android]": {
	top: "0dp"
},
"#viewEscudo": {
	height: "90dp"
},
"#imgEscudo": {
	left: "0dp",
	//height: "90dp",
	width: "250dp",
	image: "/ima/Escudo.png"
},
