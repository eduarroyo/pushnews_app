		fontFamily: "SnellRoundhand-Black",
		fontSize: "30dp"
	}
},
"#lblPoblacion[platform=android]": {
	height: "30dp",
	left: "0dp",
	width: "250dp",
	color: "white",
	textAlign: "center",
	font: {
		fontFamily: "SnellRoundhand-BlackScript",
		fontSize: "25dp"
	}
},
".rowIcon":{
	left: 5,
	top: 7,
	width: "20dp",
	height: "20dp"
},
"#viewTabla": {
	backgroundGradient: {
       type: 'linear',
       colors: [ Alloy.Globals.Menu_Color_Arriba, Alloy.Globals.Menu_Color_Abajo],
       backfillStart: true
	}
},
".menuTable":{
	separatorColor: 'transparent',
